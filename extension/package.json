{
  "name": "scu-schedule-helper-extension",
  "version": "2.3.3",
  "description": "Chrome Extension for SCU Schedule Helper",
  "main": "index.js",
  "scripts": {
    "lint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "build": "next build && node out.mjs && npm run refreshpb",
    "buildall": "npm run build && npm run refreshcs",
    "refreshcs": "npm run build --prefix react_content_scripts",
    "refreshpb": "tsc -p tsconfig.public.json && npx esbuild ./public/content_scripts/*.ts --bundle --outdir=./out/content_scripts",
    "start": "next start",
    "run": "next start",
    "dev": "next dev"
  },
  "keywords": [],
  "author": "",
  "type": "module",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/styled-engine-sc": "^6.1.6",
    "compress.js": "^2.1.2",
    "glob": "^10.2.3",
    "match-sorter": "^8.0.0",
    "next": "^14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.313",
    "@types/compress.js": "^1.1.3",
    "@types/node": "22.13.14",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/web": "^0.0.226",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "typescript": "5.8.2"
  }
}
