{
  "manifest_version": 3,
  "name": "SCU Schedule Helper",
  "version": "2.1.0",
  "description": "Course/professor rating to help with SCU schedule planning.",
  "permissions": ["storage", "identity", "notifications"],
  "host_permissions": ["https://www.ratemyprofessors.com/*"],
  "web_accessible_resources": [
    {
      "resources": ["index.html", "tab/index.html", "images/info_icon.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://www.myworkday.com/scu/*"],
      "exclude_matches": [
        "https://www.myworkday.com/scu/d/task/2998$30300.htmld"
      ],
      "js": ["content_scripts/course_tracking.js"]
    },
    {
      "matches": ["https://www.myworkday.com/scu/*"],
      "js": ["content_scripts/section_scores.js"]
    },
    {
      "matches": ["https://www.myworkday.com/scu/*"],
      "js": ["content_scripts/enrollment_statistics.js"]
    }
  ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "key": "",
  "oauth2": {
    "client_id": "583521775185-ff1gl7infgqmof69u134s6qjjerneuaq.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}
